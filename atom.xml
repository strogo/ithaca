---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>ithaca</title>
 <link href="http://ithaca.arpinum.org/atom.xml" rel="self"/>
 <link href="http://ithaca.arpinum.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ithaca.arpinum.org/</id>
 <author>
   <name>Telemachus Odysseos</name>
   <email>telemachus@arpinum.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://arpinum.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
